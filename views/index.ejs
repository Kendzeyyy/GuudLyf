<script>
    let questions = JSON.parse(document.getElementById("questions").innerText);
    let peopleGroup = null
    const container = document.querySelector("#inner-survey");
    const lefty = document.querySelector(".lefty");
    let translate = 0;
    let maxTranslate = null;
    const righty = document.querySelector(".righty");

    sendFeedback = () => {
        goTo('thankyou-page', null)
        var data = { 
            "feedback": $("textarea#feedback").val(),
            "schoolname": $("#schoolname").val(),
            "schoolclass": $("#schoolclass").val()
        }
        if (!$("textarea#feedback").val()) return
        $.ajax({
            type: "POST",
            url: "/feedback",
            data: JSON.stringify(data),
            contentType: 'application/json',
        }).then(function (data) {
            $("textarea#feedback").val("")

        })
    }
    let prev = null
    goTo = (id, prevId) => {
        $('.common-div').hide()
        $('#' + id).show()
        $('#' + id)[0].scrollIntoView(true);
        prev = prevId
    }

    goBackTo = () => {
        if (prev) {
            $('.common-div').hide()
            $('#' + prev).show()
            $('#' + prev)[0].scrollIntoView(true);
        }
    }

    highlight = (e, input) => {
        $('.group').css({ "box-shadow": "", "border": "1px solid" })
        $("#startsurvey").prop("disabled", false)
        $("#startsurvey").css({ "background-color": "#2AB7CA", "color": "white" })
        peopleGroup = input
        if (input) {
            $(e).css({ "box-shadow": "0 0 40px rgb(42, 183, 202)", "border": "2px solid" })
        }
    }

    lefty.addEventListener("click", function () {
        if (!maxTranslate) return
        if (translate == 0) {
            translate = 0
            return
        }
        translate += 100;
        container.style.transform = "translateX(" + translate + "vw" + ")";
    });

    righty.addEventListener("click", function () {
        if (!maxTranslate) return
        if (translate == -maxTranslate) {
            translate = -maxTranslate
            return
        }
        translate -= 100;
        container.style.transform = "translateX(" + translate + "vw" + ")";
    });

    var sendAnswer = function (e) {
        var data = {
            "answerId": e.target.answerId,
            "answer": e.target.answer,
            "questionId": e.target.questionId,
            "question": e.target.question,
            "schoolname": $("#schoolname").val(),
            "schoolclass": $("#schoolclass").val(),
            "peopleGroup": peopleGroup
        }
        $.ajax({
            type: "POST",
            url: "/answers",
            data: JSON.stringify(data),
            contentType: 'application/json',
        })
    }
    startServey = () => {
        $("#inner-survey").empty()
        getData(questions)

    }

    getData = (questions) => {
        container.style.transform = "translateX(" + 0 + "vw" + ")";
        translate = 0;
        maxTranslate = null
        maxTranslate = (questions.length - 1) * 100
        questions.forEach((question, index) => {
            var mainContainer = document.createElement("div")
            mainContainer.className = "mainContainer"
            mainContainer.id = "mainContainer" + question.questionId

            var header = document.createElement("div")
            header.className = 'header'

            var headerContainer = document.createElement("div");
            headerContainer.style.backgroundColor = '#F4F4F8';
            headerContainer.className = 'w-100 justify-content-center d-flex position-relative'

            var headerImg = document.createElement('img');
            headerImg.style.height = '60px'
            headerImg.style.marginTop = '10px'
            headerImg.src = 'HELSINKI_Tunnus_MUSTA.png'

            headerContainer.appendChild(headerImg)
            header.appendChild(headerContainer)


            var surveyQuestion = document.createElement("div")
            surveyQuestion.style.display = 'flex';
            surveyQuestion.style.justifyContent = 'center';
            surveyQuestion.className = 'surveyQuestion'

            var surveyQuestionContainer = document.createElement("div")
            surveyQuestionContainer.className = 'surveyQuestionContainer';
            surveyQuestionContainer.innerHTML = question.question
            surveyQuestion.appendChild(surveyQuestionContainer)


            var serveyChoices = document.createElement("div")
            serveyChoices.className = "serveyChoices"

            var serveyChoicesContainer = document.createElement("div")
            serveyChoicesContainer.className = 'serveyChoicesContainer';

            serveyChoices.appendChild(serveyChoicesContainer)

            var navigateControl = document.createElement("div")
            navigateControl.className = "navigateControl"

            var navigateControlContainer = document.createElement("div")
            navigateControlContainer.className = 'navigateControlContainer'

            var navigateControlbtn1 = document.createElement("button")
            navigateControlbtn1.className = "btn button";
            navigateControlbtn1.innerHTML = "Edellinen"
            navigateControlbtn1.addEventListener("click", function () {
                if (index == 0) {
                    goTo("peopleGroup", null)
                    $("#common-div-outer").show()
                    $("#inner-survey").empty()
                }
                else {
                    $("#left-button").click()
                }
            })

            var navigateControlspan = document.createElement("span")
            navigateControlspan.innerHTML = index + 1 + "/8";
            navigateControlspan.style.color = "#2AB7CA"
            navigateControlspan.style.margin = "10px"

            var navigateControlbtn2 = document.createElement("button")
            navigateControlbtn2.className = "btn button2";
            navigateControlbtn2.innerHTML = index == 7 ? "L채het채" : "Seuraava"
            navigateControlbtn2.disabled = "true"


            navigateControlbtn2.addEventListener("click", function (e) {
                sendAnswer(e)
                if (index == 7) {
                    $("#inner-common-div, #common-div-outer").show()
                    goTo("thankyou-page", null)
                    $("#inner-survey").empty()
                    highlight("teacher", null)

                }
                else {
                    $("#right-button").click()
                }
            })

            navigateControlContainer.appendChild(navigateControlbtn1)
            navigateControlContainer.appendChild(navigateControlspan)
            navigateControlContainer.appendChild(navigateControlbtn2)

            var h6 = document.createElement("h6")
            h6.innerHTML = "Guudlyf 2019";
            h6.className = "team-logo"
            h6.style.color = '#BBBBBB';


            navigateControl.appendChild(navigateControlContainer)

            mainContainer.appendChild(header)
            mainContainer.appendChild(surveyQuestion)
            mainContainer.appendChild(serveyChoices)
            mainContainer.appendChild(navigateControl)
            mainContainer.appendChild(h6)


            document.getElementById("inner-survey").appendChild(mainContainer);
            var choices = question.choices
            choices.map(item => {
                var choice = document.createElement("button")
                choice.className = 'button-answer'
                choice.questionId = question.questionId;
                choice.id = item.choiceId;
                var icon = document.createElement("img");
                icon.src = item.icon;

                var text = document.createElement("span");
                text.innerHTML = item.choice

                choice.appendChild(icon);
                choice.appendChild(text)
                serveyChoicesContainer.appendChild(choice)

                $(choice).click(function (event) {
                    $(navigateControlbtn2).prop("disabled", false)
                    event.preventDefault()
                    $('.button-answer').css({ "box-shadow": "", "border": "1px solid" })
                    $(event.currentTarget).css({ "box-shadow": "0 0 40px rgb(42, 183, 202)", "border": "2px solid" })
                    $(navigateControlbtn2).css({ "background-color": "#2AB7CA", "color": "white" })
                    $(navigateControlbtn2).prop({ "questionId": question.questionId, "answer": item.choice, "answerId": item.choiceId, "question": question.question })
                })
            })
        })
        $(document).scrollTo('#survey');
        $("#common-div-outer").hide()
    }
</script>
